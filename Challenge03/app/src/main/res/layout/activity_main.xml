<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/holo_blue_dark"
    android:orientation="horizontal"
    tools:context="com.example.palmdigital.challenge03.MainActivity">

    <ImageButton
        android:id="@+id/one"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="69dp"
        app:srcCompat="@drawable/numberone" />

    <ImageButton
        android:id="@+id/two"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginTop="69dp"
        android:layout_toRightOf="@id/one"
        app:srcCompat="@drawable/numbertwo" />

    <ImageButton
        android:id="@+id/three"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginTop="69dp"
        android:layout_toRightOf="@id/two"
        app:srcCompat="@drawable/numberthree" />

    <ImageButton
        android:id="@+id/four"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/one"
        android:layout_marginLeft="90dp"
        app:srcCompat="@drawable/numberfour" />

    <ImageButton
        android:id="@+id/five"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/two"
        android:layout_toRightOf="@id/four"
        app:srcCompat="@drawable/numberfive" />

    <ImageButton
        android:id="@+id/six"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/three"
        android:layout_toRightOf="@id/five"
        app:srcCompat="@drawable/numbersix"
        android:onClick="displayNumberSix"
        />
</RelativeLayout>
